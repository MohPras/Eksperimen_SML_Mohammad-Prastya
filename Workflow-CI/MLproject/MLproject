name: cs-mlflow

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 505}
      max_depth: {type: int, default: 35}
      dataset: {type: str, default: "netflix_preprocessing.csv"}
    command: >
      python modelling_tuning.py --n_estimators {n_estimators} --max_depth {max_depth} --dataset {dataset}